<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE aspectran PUBLIC "-//ASPECTRAN//DTD Aspectran Configuration 6.0//EN"
        "http://aspectran.github.io/dtd/aspectran-6.dtd">
<aspectran>

    <description>
        A set of voices used in Skylark
    </description>

    <bean id="voice-kevin16" class="com.aspectran.skylark.tts.TextToSpeechBean">
        <properties>
            <item name="properties" type="properties">
                <value name="freetts.voices">com.sun.speech.freetts.en.us.cmu_us_kal.KevinVoiceDirectory</value>
            </item>
            <item name="voiceName">kevin16</item>
        </properties>
    </bean>

</aspectran>